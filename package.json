    "dev:classic:android": "yarn workspace classic android",
    "format": "prettier --write .",
    "postinstall": "expo-yarn-workspaces postinstall"
  },
  "devDependencies": {
    "prettier": "^3.6.2"
  },
  "dependencies": {
    "expo-yarn-workspaces": "^2.3.2"
  }
